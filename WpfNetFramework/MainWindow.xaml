        xmlns:el="clr-namespace:ElementUI.Wpf;assembly=ElementUI.Wpf"
        xmlns:local="clr-namespace:WpfNetFramework"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Horizontal">
            <el:ELButton Content="Default" Type="Default"></el:ELButton>
            <el:ELButton Content="Primary" Type="Primary"></el:ELButton>
            <el:ELButton Content="Success" Type="Success"></el:ELButton>
            <el:ELButton Content="Info" Type="Info"></el:ELButton>
            <el:ELButton Content="Warning" Type="Warning"></el:ELButton>
            <el:ELButton Content="Danger" Type="Danger"></el:ELButton>
        </StackPanel>
        
        <StackPanel Orientation="Horizontal">
            <el:ELButton Content="Default" Plain="True" Type="Default"></el:ELButton>
            <el:ELButton Content="Primary" Plain="True" Type="Primary"></el:ELButton>
            <el:ELButton Content="Success" Plain="True" Type="Success"></el:ELButton>
            <el:ELButton Content="Info" Plain="True" Type="Info"></el:ELButton>
            <el:ELButton Content="Warning" Plain="True" Type="Warning"></el:ELButton>
            <el:ELButton Content="Danger" Plain="True" Type="Danger"></el:ELButton>
        </StackPanel>
        
